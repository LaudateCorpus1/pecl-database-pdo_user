  <name>Sara Golemon</name>
  <user>pollita</user>
  <email>pollita@php.net</email>
  <active>yes</active>
 </lead>

   <date>2006-05-10</date>
   <version>
    <release>0.2.0</release>
    <api>0.2.0</api>
   </version>
   <stability>
    <release>beta</release>
    <api>beta</api>
   </stability>
